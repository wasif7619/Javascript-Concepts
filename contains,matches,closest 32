let a1=document.getElementById('id1')
console.log(a1)

console.log(a1.matches('.class'))  //matches function check if exist or not 

console.log(a1.closest('id2'))  //this is going to check from this id2 to back to parent like if exist of not 

let a2=document.getElementById('sp1')
console.log(a2.contains(a2)) 
